<?xml version="1.0" encoding="utf-8"?> 
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Label="UserMacros">
    <MARGAVIT_ROOT>$(SolutionDir)..\..\</MARGAVIT_ROOT>
    <MARGAVIT_BUILD_SCRIPTS>$(MARGAVIT_ROOT)Scripts\Build\</MARGAVIT_BUILD_SCRIPTS>
    <MARGAVIT_AUTO_GENERATED>$(MARGAVIT_ROOT)Common\AutoGenerated\</MARGAVIT_AUTO_GENERATED>
    <DETOURS_PATH>$(MARGAVIT_ROOT)Deps\ThirdParty\Detours\</DETOURS_PATH>
  </PropertyGroup>
  <ItemGroup>
    <BuildMacro Include="MARGAVIT_ROOT">
      <Value>$(MARGAVIT_ROOT)</Value>
    </BuildMacro>
    <BuildMacro Include="MARGAVIT_BUILD_SCRIPTS">
      <Value>$(MARGAVIT_ROOT)Scripts\Build</Value>
    </BuildMacro>
    <BuildMacro Include="MARGAVIT_AUTO_GENERATED">
      <Value>$(MARGAVIT_ROOT)Common\AutoGenerated\</Value>
    </BuildMacro>
    <BuildMacro Include="DETOURS_PATH">
      <Value>$(DETOURS_PATH)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>
